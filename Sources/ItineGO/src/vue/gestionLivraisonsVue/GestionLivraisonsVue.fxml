<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vue.gestionLivraisonsVue.GestionLivraisonsVue">
   <center>
      <BorderPane minWidth="184.0" prefHeight="507.0" prefWidth="380.0" BorderPane.alignment="CENTER">
         <top>
            <Label text="Plan de ville" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </top>
         <center>
            <StackPane fx:id="planVillePane" minHeight="370.0" minWidth="211.0" prefHeight="434.0" prefWidth="277.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin></StackPane>
         </center>
         <BorderPane.margin>
            <Insets bottom="10.0" top="10.0" />
         </BorderPane.margin>
      </BorderPane>
   </center>
   <left>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <top>
            <VBox maxWidth="400.0" minWidth="400.0" prefWidth="400.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox maxWidth="400.0" minWidth="400.0" prefHeight="27.0" prefWidth="400.0">
                     <children>
                        <ImageView fx:id="imageViewAccueil" fitHeight="32.0" fitWidth="32.0" onMouseClicked="#home" onMouseEntered="#imageViewAccueilEntered" onMouseExited="#imageViewAcceuilExited" pickOnBounds="true" preserveRatio="true">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </ImageView>
                        <ImageView fx:id="imageViewPrecedent" fitHeight="32.0" fitWidth="32.0" onMouseClicked="#precedent" onMouseEntered="#imageViewPrecedentEntered" onMouseExited="#imageViewPrecedentExited" pickOnBounds="true" preserveRatio="true" />
                        <Label fx:id="labelError" alignment="CENTER" prefHeight="27.0" prefWidth="341.0" text="Error" textAlignment="CENTER" textFill="RED" />
                     </children>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </HBox>
                  <Label fx:id="labelEntrepot" maxWidth="400.0" minWidth="400.0" prefWidth="400.0" text="Adresse de l'entrepôt :">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="labelHorraires" text="adresse">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
               </children>
               <BorderPane.margin>
                  <Insets left="5.0" right="5.0" />
               </BorderPane.margin></VBox>
         </top>
         <center>
            <TableView fx:id="livraisonTable" prefHeight="295.0" prefWidth="400.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="adresseColonne" prefWidth="106.0" text="Adresse" />
                <TableColumn prefWidth="155.0" text="Plages">
                     <columns>
                        <TableColumn fx:id="plageDebutColonne" prefWidth="90.0" text="Début" />
                        <TableColumn fx:id="plageFinColonne" prefWidth="89.0" text="Fin" />
                     </columns></TableColumn>
                  <TableColumn fx:id="dureeColonne" prefWidth="115.0" text="Durée" />
              </columns>
               <BorderPane.margin>
                  <Insets left="5.0" right="5.0" />
               </BorderPane.margin>
            </TableView>
         </center>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" top="10.0" />
         </BorderPane.margin>
         <bottom>
            <VBox prefHeight="63.0" prefWidth="410.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="boutonCalculer" maxHeight="27.0" maxWidth="413.0" minHeight="27.0" minWidth="400.0" mnemonicParsing="false" onMouseClicked="#calculerLivraisonAction" prefHeight="27.0" prefWidth="413.0" text="Calculer tournée" />
