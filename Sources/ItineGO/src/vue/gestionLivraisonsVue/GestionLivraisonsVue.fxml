                     <children>
                        <ProgressBar fx:id="barreChargement" prefHeight="20.0" prefWidth="185.0" progress="0.0" />
                        <Label fx:id="labelTempsRestant" maxWidth="156.0" minWidth="156.0" prefWidth="156.0" text="Temps restant : 1000 sec">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <Button fx:id="boutonStopCalculer" mnemonicParsing="false" onMouseClicked="#stopperCalculLivraisonAction" prefHeight="27.0" prefWidth="58.0" text="Stop" />
                     </children>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
            </VBox>
         </bottom></BorderPane>
   </left>
</BorderPane>
